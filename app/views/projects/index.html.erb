<p style="color: green"><%= notice %></p>

<h1>Projects</h1>

<div id="projects">
  <% @projects.each do |project| %>
    <%= render project %>
    <p>
      <%= link_to "Show this project", project %>
    </p>
  <% end %>
</div>

<% if user_signed_in? %>
  <%= link_to "New project", new_project_path %>
<% else %>
  <%= link_to "Sign in", user_session_path %>
<% end %>





